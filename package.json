{
  "name": "dependency-injection",
  "version": "1.0.0",
  "description": "A JavaScript dependency injection container",
  "main": "dist/dependency.js",
  "scripts": {
    "build": "npm run lint && npm run clean && npm run build:dist",
    "build:dist": "node_modules/.bin/babel src --out-dir dist --source-maps --minified",
    "clean": "node_modules/.bin/rimraf ./dist",
    "lint": "node_modules/.bin/eslint --color --format stylish src demo",
    "start": "node_modules/.bin/babel-node demo/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subhajitdas/dependency-injection.git"
  },
  "keywords": [
    "dependency",
    "injection",
    "es6",
    "fluent"
  ],
  "author": "Subhajit Das <subhajit@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subhajitdas/dependency-injection/issues"
  },
  "homepage": "https://github.com/subhajitdas/dependency-injection#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.5.0",
    "eslint-plugin-import": "^2.7.0",
    "rimraf": "^2.6.1"
  }
}