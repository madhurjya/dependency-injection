{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Demo",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/demo/index.js",
            "stopOnEntry": false,
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "production"
            }
        }
    ]
}